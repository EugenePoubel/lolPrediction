            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4.3"
        },
        "sort-packages": true
    },
    "scripts": {
        "start": "symfony server:start --no-tls",
        "stop": "symfony server:stop"
